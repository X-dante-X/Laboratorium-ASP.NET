@using Laboratorium_3.Models.ReservationModels;
@model Reservation

<h1>
    Szczegóły
</h1>
<h3>Wlasciciel: @Model.ContactName</h3>
<h3>Data - @Model.Data.ToShortDateString()</h3>
<h3>Miasto - @Model.Miasto</h3>
<h3>Adres - @Model.Adress</h3>
<h3>Nazwa pokoju - @Model.PokojNazwa</h3>
<h3>Numer pokoju - @Model.PokojNumer</h3>
<h3>Rozmiar pokoju - @Model.PokojRozmiar</h3>
<h3>Pietro pokoju - @Model.PokojPietro</h3>
<h3>Cena - @Model.Cena</h3>
<a class="btn btn-primary" asp-action="Index">Powrót</a>